{
  "name": "@motorghar-platform/fastify-gateway",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "fastify-gateway",
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "platform": "node",
          "outputPath": "apps/fastify-gateway/dist",
          "format": [
            "cjs"
          ],
          "bundle": false,
          "main": "apps/fastify-gateway/src/main.ts",
          "tsConfig": "apps/fastify-gateway/tsconfig.app.json",
          "assets": [
            "apps/fastify-gateway/src/assets"
          ],
          "esbuildOptions": {
            "sourcemap": true,
            "outExtension": {
              ".js": ".js"
            }
          }
        },
        "configurations": {
          "development": {},
          "production": {
            "esbuildOptions": {
              "sourcemap": false,
              "outExtension": {
                ".js": ".js"
              }
            }
          }
        }
      },
      "prune-lockfile": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "executor": "@nx/js:prune-lockfile",
        "outputs": [
          "{workspaceRoot}/apps/fastify-gateway/dist/package.json",
          "{workspaceRoot}/apps/fastify-gateway/dist/pnpm-lock.yaml"
        ],
        "options": {
          "buildTarget": "build"
        }
      },
      "copy-workspace-modules": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "outputs": [
          "{workspaceRoot}/apps/fastify-gateway/dist/workspace_modules"
        ],
        "executor": "@nx/js:copy-workspace-modules",
        "options": {
          "buildTarget": "build"
        }
      },
      "prune": {
        "dependsOn": [
          "prune-lockfile",
          "copy-workspace-modules"
        ],
        "executor": "nx:noop"
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "fastify-gateway:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "fastify-gateway:build:development"
          },
          "production": {
            "buildTarget": "fastify-gateway:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  },
  "dependencies": {
    "fastify": "~5.2.1",
    "fastify-plugin": "~5.0.1",
    "@fastify/autoload": "~6.0.3",
    "@fastify/sensible": "~6.0.2",
    "@motorghar-platform/constants": "workspace:*",
    "@motorghar-platform/config": "workspace:*",
    "@motorghar-platform/auth": "workspace:*",
    "@motorghar-platform/types": "workspace:*",
    "@motorghar-platform/database": "workspace:*",
    "@motorghar-platform/fastify-plugins": "workspace:*",
    "@motorghar-platform/auth-service": "workspace:*",
    "@motorghar-platform/rbac-service": "workspace:*",
    "@motorghar-platform/repositories": "workspace:*"
  }
}
